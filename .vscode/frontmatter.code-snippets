{
  "Frontmatter Template": {
    "scope": "markdown",
    "prefix": "set frontmatter",
    "body": [
      "---",
      "isDraft: true",
      "title: $1",
      "description: $2",
      "date: $3",
      "heroImage: \"/images/$4.jpg\"",
      "heroAlt: $5",
      "#head:",
      "#  - [",
      "#      meta,",
      "#      {",
      "#        rel: canonical,",
      "#        href: url,",
      "#      },",
      "#    ]",
      "meta:",
      "  keywords:",
      "    - $6",
      "---"
    ],
    "description": "Set fronmatter to the new post"
  },
